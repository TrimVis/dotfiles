TOUT=$(xset q | grep "DPMS is" | awk '{ print $3 }')

case $BLOCK_BUTTON in
    1) if [ "$TOUT" = "Disabled" ]
    then
				xset s on +dpms
				TOUT="Enabled"
    else
				xset s off -dpms
				TOUT="Disabled"
    fi
esac

