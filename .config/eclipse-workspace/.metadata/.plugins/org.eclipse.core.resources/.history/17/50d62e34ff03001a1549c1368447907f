package tutorial;

public class Kreis {
	int x;
	public int y;
	public double radius;
	public String name;
	private double pi = 3.1418;
	
	Kreis(int x, int y, double radius) {
		this.x = x;
		this.y = y;
		this.radius = radius;
		this.name = "leererString";
	}
	
	public double getArea(double factor) {
		return factor * pi * (double)radius * (double)radius;
	}
	

}
