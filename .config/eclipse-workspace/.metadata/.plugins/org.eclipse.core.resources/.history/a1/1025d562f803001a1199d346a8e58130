package tutorial;

public class Kreis {
	private int x;
	private int y;
	private double radius;
	private String name;
	private double pi = 3.1418;
	
	Kreis(int x, int y, double radius) {
		this.x = x;
		this.y = y;
		this.radius = radius;
		name = "leererString"
	}
	
	public double getArea(double factor) {
		return factor * pi * (double)radius * (double)radius;
	}
	

}
